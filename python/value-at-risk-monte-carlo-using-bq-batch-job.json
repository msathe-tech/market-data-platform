{
    "taskGroups": [
        {
            "taskSpec": {
                "runnables": [
                    {
                        "container": {
                            "imageUri": "us-central1-docker.pkg.dev/duet-1/market-data/var-mc-bq",
                            "entrypoint": "python",
                            "commands": [
                                "app.py",
                                "--DAYS=30",
                                "--YEARS=23"
                            ]
                        }
                    }
                ],
                "computeResource": {
                    "cpuMilli": 2000,
                    "memoryMib": 2000
                },
                "maxRetryCount": 0
            },
            "taskCount": 200,
            "parallelism": 200
        }
    ],
    "allocationPolicy": {
        "network": {
            "networkInterfaces": [
            {
              "network": "global/networks/default",
              "subnetwork": "regions/us-central1/subnetworks/default",
              "noExternalIpAddress": true
            }
           ]
        },
        
        "instances": [
            {
                "policy": { "machineType": "e2-standard-8" }
            }
        ]
    },
    "labels": {
        "department": "finance",
        "env": "testing"
    },
    "logsPolicy": {
        "destination": "CLOUD_LOGGING"
    }

}