{
    "taskGroups": [
        {
            "taskSpec": {
                "runnables": [
                    {
                        "container": {
                            "imageUri": "us-central1-docker.pkg.dev/duet-1/market-data/var-mc-bq",
                            "commands": [
                                "python", 
                                "app.py"
                            ]
                        }
                    }
                ],
                "computeResource": {
                    "cpuMilli": 2000,
                    "memoryMib": 10000
                },
                "maxRetryCount": 0
            },
            "taskCount": 2,
            "parallelism": 2
        }
    ],
    "allocationPolicy": {
        "network": {
            "networkInterfaces": [
            {
              "network": "global/networks/default",
              "subnetwork": "regions/us-central1/subnetworks/default",
              "noExternalIpAddress": true
            }
           ]
        },
        
        "instances": [
            {
                "policy": { "machineType": "e2-standard-8" }
            }
        ]
    },
    "labels": {
        "department": "finance",
        "env": "testing"
    },
    "logsPolicy": {
        "destination": "CLOUD_LOGGING"
    }

}