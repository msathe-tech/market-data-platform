{
    "taskGroups": [
        {
            "taskSpec": {
                "runnables": [
                    {
                        "container": {
                            "imageUri": "us-central1-docker.pkg.dev/duet-1/market-data/var-mc-bq",
                            "entrypoint": "python",
                            "commands": [
                                "ValueAtRisk-VaR-MonteCarlo-UsingBQ.py",
                                "--DAYS=25",
                                "--YEARS=22"
                            ]
                        }
                    }
                ],
                "computeResource": {
                    "cpuMilli": 2000,
                    "memoryMib": 10000
                },
                "maxRetryCount": 0
            },
            "taskCount": 1,
            "parallelism": 1
        }
    ],
    "allocationPolicy": {
        "network": {
            "networkInterfaces": [
            {
              "network": "global/networks/default",
              "subnetwork": "regions/us-central1/subnetworks/default",
              "noExternalIpAddress": true
            }
           ]
        },
        
        "instances": [
            {
                "policy": { "machineType": "n2-highcpu-2" }
            }
        ]
    },
    "labels": {
        "department": "finance",
        "env": "testing"
    },
    "logsPolicy": {
        "destination": "CLOUD_LOGGING"
    }

}